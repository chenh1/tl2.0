{
  "name": "create-next-example-app",
  "scripts": {
    "dev": "next",
    "build": "next build && npm run copy-sw-dev",
    "start": "next start",
    "build:static": "next export && npm run copy-sw-prod",
    "copy-sw-dev": "node ./pwa/exportFiles.dev.js",
    "copy-sw-prod": "node ./pwa/exportFiles.prod.js",
    "test": "jest",
    "server-side": "npm run build && npm run start",
    "get-pid-3000": "lsof -i tcp:3000",
    "kill-3000": "kill -9 `lsof -i TCP:3000 | awk '/LISTEN/{print $2}'`"
  },
  "dependencies": {
    "merge": ">=1.2.1",
    "next": "6.1.2",
    "react": "16.5.0",
    "react-dom": "16.5.0",
    "recompose": "^0.30.0"
  },
  "devDependencies": {
    "jest": "^23.5.0",
    "react-test-renderer": "^16.7.0"
  }
}
